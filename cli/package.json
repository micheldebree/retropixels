{
  "name": "retropixels",
  "version": "0.8.6",
  "description": "A cross platform command line tool to convert images to Commodore 64 format.",
  "bin": {
    "retropixels": "./cli.js"
  },
  "scripts": {
    "build": "npx tsc",
    "lint": "npx eslint '*/**/*.{js,ts}' --quiet -c .eslintrc.json --no-eslintrc",
    "fix": "npx eslint '*/**/*.{js,ts}' --quiet --fix -c .eslintrc.json --no-eslintrc && npx prettier cli.js --write"
  },
  "dependencies": {
    "commander": "7.2.0",
    "fs-extra": "10.0.0",
    "retropixels-core": "0.1.2",
    "path": "0.12.7"
  },
  "devDependencies": {
    "c64jasm": "0.9.2",
    "del": "6.0.0",
    "dpdm": "3.6.1",
    "eslint": "7.26.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "madge": "4.0.2",
    "prettier": "2.2.1",
    "retire": "2.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micheldebree/retropixels.git"
  },
  "author": "Michel de Bree",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micheldebree/retropixels/issues"
  },
  "homepage": "https://github.com/micheldebree/retropixels#readme"
}
